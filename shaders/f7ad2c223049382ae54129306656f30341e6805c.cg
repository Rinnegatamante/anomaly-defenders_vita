// profile sce_fp_psp2

#define POSTPROCESSING_ENABLED
#define _ANDROID

uniform sampler2D Texture0;


void main(
	float2 uvVarying : TEXCOORD0,
	float4 colorVarying : COLOR,
	float4 out gl_FragColor : COLOR
) {
	float4 color = colorVarying;
	
#ifdef TEXTURE
	float4 txt = tex2D( Texture0, uvVarying );
	color *= txt;
#endif

    gl_FragColor = color;
}

